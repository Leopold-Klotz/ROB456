<!--
	Example launch file, publish a circling point, and show it in rviz.

	Bill Smart, smartw@oregonstate.edu
-->


<launch>

	<!-- Start the circle node -->
	<node pkg="lab0_r2" name="circler" type="circler.py" />

	<!-- Start rviz -->
	<node pkg="rviz" name="rviz" type="rviz" args="-d $(find lab0_r2)/config/circler.rviz" />

</launch>
